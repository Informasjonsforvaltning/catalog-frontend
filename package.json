{
  "name": "catalog-frontend",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "start:concept": "nx serve concept-catalog",
    "start:service": "nx serve service-catalog",
    "start:data-service": "nx serve data-service-catalog",
    "build": "nx build",
    "build-all": "nx run-many --target=build --all",
    "test": "yarn run affected:test",
    "test-all": "nx run-many --target=test --all",
    "lint": "yarn run affected:lint",
    "lint-all": "yarn nx run-many --target=lint --skip-nx-cache --verbose",
    "affected:build": "nx affected -t build",
    "affected:e2e": "nx affected -t e2e",
    "affected:test": "nx affected -t test --exclude=ui-v2",
    "affected:lint": "nx affected -t lint",
    "affected": "nx affected",
    "affected:matrix": "nx show projects --affected --type=app | tr '\n' ',' | jq -cR 'split(\",\")[:-1]'",
    "all:matrix": "nx show projects --type=app | tr '\n' ',' | jq -cR 'split(\",\")[:-1]'",
    "matrix": "[ -z \"$(yarn run affected:matrix)\" ] && yarn run all:matrix || yarn run affected:matrix",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepare": "husky",
    "postinstall": "husky install",
    "graphql:codegen": "graphql-codegen --config codegen.ts"
  },
  "private": true,
  "workspaces": [
    "libs/ui-v2"
  ],
